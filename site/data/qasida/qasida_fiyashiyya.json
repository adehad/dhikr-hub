    "verses": {
        "ara": [
            ["أَنَا عَبْدُ رَبِّي لَهُ قُدْرَةٌ"],
            ["يَهُونُ بِهَا كُلُّ أَمْرٍ عَسِيرْ"],
            ["فَإِنْ كَنْتُ عَبْدً ضَعِيفَ الْقُوَّى"],
            ["فَرَبِّي عَلَى كُلِّ شَيْءٍ قَدِيرْ"],

            ["مِنِّي اَشَ عْلَيَّا"],
            ["وَنَا عَبْدٌ مَمْلُوكْ"],

            ["وَالْأَشْيَا مَقْضِيَّا"],
            ["مَا فِى التَّحْقِيقْ شُكُوكْ"],

            ["رَبِّي نَاظَرْ فِيَّا"],
            ["وَنَا نَظْرِي مَتْرُوكْ"],

            ["فِي الْأَرْحَمْ وَالْحَاشَا"],
            ["مِنْ نُّطْفَا صَوَّرْنِي"],

            ["أَنَا مَالِي فِيَاشْ"],
            ["اَشَ عْلَيَّا مِنِّي"],

            ["نَقْلَقْ مِنْ رِّزٔقِي لَاشْ"],
            ["وَالْخَالِقْ يَرْزَقْنِي"],

            ["اللّهُمَّ صَلِّ عَلَى الْمُصْطَفَى"],
            ["حَبِيبْنَا مُحَمَّدْ عَلَيْهِ السَّلَام"],

            ["يَقُولُ لِمَا شَاءَ كُنْ فَيَكُونْ"],
            ["وَيُبْدِءُ سُبْحَانَهُ وَيُعِيدْ"],
